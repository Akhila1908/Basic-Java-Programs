import java.util.*;
public class Main {
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        System.out.println(sum(n));
    }
    public static int sum(int n){
        int sum=0;
        while(n>0){
        int rem=n%10;
        sum=rem+sum;
        n/=10;}
      return sum;
    }
}
