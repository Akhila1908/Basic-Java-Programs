class Main {
    public static void main(String[] args) {
        int n=2,x=10;
       power(n,x);
       }
       public static void power(int n,int x){
            int ans=1;
           while(x>0){
        if(x%2!=0){  
            ans*=n;
            x--;
        }
        else{
           x/=2;
           n=n*n;
        }}
        System.out.println(ans);
    }
}
