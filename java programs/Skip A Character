import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        String s="baccad";
        char ch='a';
 System.out.println(removeCh(s,ch));}
public static String removeCh(String s,char ch){
StringBuilder str=new StringBuilder();
for(int i=0;i<s.length();i++){
    if(s.charAt(i)!=ch){
       str.append(s.charAt(i));
    }
}
 return str.toString();
    }
}
