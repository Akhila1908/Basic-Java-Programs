  
import java.util.*;
public class Main {
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        sum(n);
    }
    public static void sum(int n){
        if(n%10==n){
            System.out.println(n);
            return;
             }
        System.out.print(n%10);
        sum(n/10);
    }
         }
