import java.io.*;
public class Solution {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String str=s.next();
    Stack<Character> stk=new Stack<>();
        for(int i=0;i<str.length();i++){
            stk.push(str.charAt(i));
        }
    String ans="";
        while(!stk.empty()){
        ans+=stk.pop();}
        System.out.println(ans);
    }
}
